<?xml version="1.0" encoding="utf-8"?>
<resources>

    <public name="TwowaySeekBarStyle" type="style" />
    <style name="TwowaySeekBarStyle">
        <item name="android:background">@drawable/twowayseekbar_background</item>
        <item name="track">@drawable/twowayseekbar_track</item>
        <item name="progress">@drawable/twowayseekbar_track_progress</item>
        <item name="tickMark">@drawable/twowayseekbar_tick_mark</item>
        <item name="android:thumb">@drawable/twowayseekbar_thumb</item>

        <item name="enableTickMarks">false</item>
        <item name="splitTrack">false</item>

        <item name="android:paddingStart">16dp</item>
        <item name="android:paddingEnd">16dp</item>

        <item name="android:focusable">true</item>
        <item name="android:mirrorForRtl">true</item>
    </style>

</resources>